<Application x:Class="Projekt_Dokumentenablage.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Projekt_Dokumentenablage"
             xmlns:controls="clr-namespace:Projekt_Dokumentenablage.Controls"
             xmlns:vm="clr-namespace:Projekt_Dokumentenablage.ViewModels"
             xmlns:views="clr-namespace:Projekt_Dokumentenablage.View"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:PasswordRequestViewModel}">
            <controls:PasswordRequestControl/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:MainVM}">
            <controls:MainControl/>
        </DataTemplate>
    </Application.Resources>
</Application>
